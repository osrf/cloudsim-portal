{
  "env": {
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {"ecmaVersion": 6},
  "rules": {
    "indent": ["warn", 2],
    "linebreak-style": [
      "warn",
      "unix"
    ],
    "no-unused-vars": ["warn", { "vars": "all", "args": "after-used" }],
    "semi": "off",
    "no-console": 0,
    "no-undef": "off",
    "no-negated-in-lhs": 0
  }
}
